  "description": "This is a extension for Magento Community Edition that adds the possibility to use dotenv environment variables in the local.xml file.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    },
    {
      "name": "Ross Golder"
    }
  ],
  "type": "magento-module",
  "require": {
    "Ffuenf/Ffuenf_Common": "*",
    "Ffuenf/Ffuenf_MageTrashApp": "*",
    "vlucas/phpdotenv": "2.*"
